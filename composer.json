{
    "name": "rad/rad-framework",
    "description": "Rest API Dedicated Framework.",
    "authors": [
        {
            "name": "Guillaume Monet",
            "email": "guillaume.monet@free.fr"
        }
    ],

    "license": "MIT",

    "autoload": {
        "psr-4": {
            "Rad\\": "src/Rad/"
        }
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "rad/rad-framework",
                "version": "dev-master",
                "source": {
                    "url": "git://github.com/guillaumemonet/rad.git",
                    "type": "git",
                    "reference": "master"
                },
                "autoload": {
                    "psr-4": {
                        "Rad\\": "src/Rad/"
                    }
                }
            }
        }
    ],
    "require": {
        "php": " ^7.1",
        "psr/log": "^1",
        "psr/cache": "^1",
        "psr/container": "^1",
        "psr/http-message": "^1",
        "psr/link": "^1",
        "psr/simple-cache": "^1.0",
        "smarty/smarty": "^v3.1"
    },
    "require-dev": {
        "phpunit/php-timer": "^1.0.",
        "phpunit/phpunit": "^6.5"
    },
    "scripts": {
        "create-project": [
            "Rad\\Composer\\Manager::createProject"
        ]
    }
}
